using System;
					
public class Program
{
	public static void PrzedziałAB()
	{
		double A, B, X;

		// wczytywanie wartości dla A, B, X
		Console.WriteLine("Podaj wartość A = ");
		A = Convert.ToDouble(Console.ReadLine());
		Console.WriteLine("Podaj wartość B = ");
		B = Convert.ToDouble(Console.ReadLine());
		Console.WriteLine("Podaj wartość X = ");
		X = Convert.ToDouble(Console.ReadLine());
 
		//sumuje cyfry podanej liczby calkowitej
		bool wynik; //true or false
		wynik = (A<X && X<B) || (B<X && X<A);

		if (wynik == true)
		{			
			Console.WriteLine("Podana wartość X = {0} należy do przedziału ({1}, {2}).", X, A, B);
		}
		else if (wynik == true & A>B)
		{
			Console.WriteLine("Podana wartość X = {0} należy do przedziału ({2}, {1}).", X, A, B);
		}
		else if (wynik == false & A<B)
		{
			Console.WriteLine("Podana wartość X = {0} nie należy do przedziału ({1}, {2}).", X, A, B);
		}
		else if (wynik == false & A>B)
		{
			Console.WriteLine("Podana wartość X = {0} nie należy do przedziału ({2}, {1}).", X, A, B);
		}			
 
	}	
	public static void Main(string[] args)
	{
		//wywołuje funkcję
		PrzedziałAB();
	}
}
