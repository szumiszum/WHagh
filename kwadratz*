using System;
					
public class Program
{
	public static void Main()
	{
		int n;
		Console.WriteLine("Podaj rozmiar kwadratu: "); 
		n = int.Parse(Console.ReadLine()); // konwersja
		
		Console.WriteLine("Rozmiar kwadratu wynosi: {0}", n);
		
		for (int i = 0; i < n; i++)
		{
			// Console.Write('*');
			
			for (int j = 0; j < n; j++)
			{
				Console.Write('*');
			}
			
			Console.WriteLine(); // przejÅ›cie do nowej linii
		}
		
		
	}
}
