using System;
					
public class Prostokąt
{
	private int szerokość;
	private int wysokość;
	
	public Prostokąt(int x, int y) //konstruktor
	{
		szerokość = x;
		wysokość = y;
	}
	
	private int powierzchnia()
	{
		int z = szerokość * wysokość;
		return z;
	}
	
	private int obwod()
	{
		int w = 2*szerokość + 2*wysokość;
		return w;
	}	
	
	public int Prezentuj()
	{		
		Console.Write("Powierzchnia prostokątu wynosi");
		return powierzchnia();
	}
 
}

public class Program
{	
	public static void Main(string[] args)
	{
		int a,b;
		Console.Write("Podaj bok a: ");
		a = int.Parse(Console.ReadLine());
		Console.Write("Podaj bok b: ");
		b = int.Parse(Console.ReadLine());
		
		//definiuje obiekt
		Prostokąt p = new Prostokąt(a,b);
		//wywołuje funkcję
		p.Prezentuj();
	}
}
