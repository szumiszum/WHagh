using System;
					
public class Program
{
	public static void Wielkie(string[] tab)
	{
		for (int i = 0; i < tab.Length; i++)
		{
			tab[i] = tab[i].ToUpper();
		}
	}
	public static void Main(string[] args)
	{
		string[] tab1 = { "jeden", "dwa", "trzy" };
		Wielkie(tab1);
		for (int i = 0; i < tab1.Length; i++)
		{
			Console.Write(tab1[i] + " ");
		}

	}

}
